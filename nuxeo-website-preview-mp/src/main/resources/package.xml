<package type="addon" name="nuxeo-website-preview" version="@VERSION@">
  <title>Nuxeo TO_BE_DONE</title>
  <description>
    <p>This addon activates the Nuxeo Website Preview addon.</p>
    <p>This addon adds new features as explained <a href="https://github.com/nuxeo-sandbox/nuxeo-website-preview">here</a></p>
  </description>
  <home-page>https://github.com/nuxeo-sandbox/nuxeo-website-preview</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <hotreload-support>false</hotreload-support>
  <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance>
  <nuxeo-validation>waiting_nuxeo_certified</nuxeo-validation>
  <production-state>development</production-state>
  <supported>false</supported>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-HF</platform>
    <platform>cap-7.10</platform>
    <platform>cap-7.10-HF*</platform>
    <platform>cap-8.3</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-jsf-ui</package>
  </dependencies>
  <license>LGPL</license>
  <license-url>http://www.gnu.org/licenses/lgpl.html</license-url>
</package>
